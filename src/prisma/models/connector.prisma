enum ConnectorStatus {
    not_connected
    connected
    error
}

model Connector {
    id             String          @id @default(cuid())
    key            String          @unique
    name           String
    status         ConnectorStatus @default(not_connected)
    lastError      String?
    connectedAt    DateTime?
    disconnectedAt DateTime?
    createdAt      DateTime        @default(now())
    updatedAt      DateTime        @updatedAt
}
